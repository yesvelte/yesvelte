{
	"name": "yesvelte",
	"version": "0.0.2-next.206",
	"description": "YeSvelte UI component library for Svelte",
	"sideEffects": false,
	"keywords": [
		"svelte",
		"sveltejs",
		"bootstrap",
		"tabler",
		"svelte tabler",
		"svelte bootstrap",
		"svelte components",
		"svelte ui components",
		"svelte-kit",
		"sveltekit",
		"yesvelte",
		"tabler"
	],
	"homepage": "https://www.yesvelte.com",
	"bugs": {
		"url": "https://github.com/yesvelte/yesvelte/issues",
		"email": "ap@momentaj.com"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/yesvelte/yesvelte.git"
	},
	"license": "MIT License",
	"author": "Amir Pournasserian <ap@momentaj.com>",
	"scripts": {
		"dev": "vite dev",
		"build": "npm run style && svelte-kit sync && svelte-package",
		"build:site": "npm run style && vite build",
		"publish": "npm run build && npm publish ./package/",
		"prepublishOnly": "echo 'Did you mean to publish `./package/`, instead of `./`?' && exit 1",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --plugin-search-dir . --check . && eslint .",
		"format": "prettier --plugin-search-dir . --write .",
		"tailwind": "tailwindcss -c src/scss/tailwind/tailwind.config.cjs -i src/scss/tailwind/tailwind.css -o src/styles/tailwind.css",
		"style": "node scripts/styles.generator.js"
	},
	"type": "module",
	"peerDependencies": {
		"svelte": "^3.59.1 || ^4.0.0"
	},
	"devDependencies": {
		"@sveltejs/adapter-static": "^2.0.2",
		"@sveltejs/kit": "^1.20.4",
		"@sveltejs/package": "^1.0.0",
		"@tabler/core": "^1.0.0-beta19",
		"@tailwindcss/typography": "^0.5.9",
		"@types/bootstrap": "^5.2.6",
		"@types/inputmask": "^5.0.3",
		"@types/prismjs": "^1.26.0",
		"@types/quill": "^2.0.10",
		"@typescript-eslint/eslint-plugin": "^5.59.8",
		"@typescript-eslint/parser": "^5.59.8",
		"cssnano": "^6.0.1",
		"daisyui": "^3.0.1",
		"eslint": "^8.42.0",
		"eslint-config-prettier": "^8.8.0",
		"eslint-plugin-svelte3": "^4.0.0",
		"postcss-filter-rules": "^0.6.2",
		"prettier": "^2.8.8",
		"prettier-plugin-svelte": "^2.10.1",
		"prismjs": "^1.29.0",
		"sass": "^1.62.1",
		"svelte-check": "^3.4.3",
		"svelte-preprocess": "^5.0.4",
		"tslib": "^2.5.3",
		"typescript": "^5.1.3",
		"vite": "^4.3.9",
		"vite-plugin-watch-and-run": "^1.1.3"
	},
	"dependencies": {
		"@floating-ui/dom": "^1.2.9",
		"focus-trap": "^7.4.3",
		"fuzzy": "^0.1.3",
		"iconify-icon": "^1.0.7",
		"inputmask": "^5.0.8",
		"litepicker": "^2.0.12",
		"moment": "^2.29.4",
		"nouislider": "^15.7.0",
		"param-case": "^3.0.4",
		"quill": "^1.3.7"
	}
}
